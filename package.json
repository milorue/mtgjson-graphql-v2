{
  "name": "mtgjson-graphql-v2",
  "version": "1.1.0",
  "description": "A GraphQL API for mtgjson.com",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "drop-database": "ts-node src/util/ingest/commands/UpdateData.ts",
    "rebuild-database": "ts-node src/util/ingest/commands/UpdateData.ts && npm run ingest-database",
    "ingest-database": "npm run ingest-cards && npm run ingest-setlist && npm run ingest-sets && npm run ingest-decklist && npm run ingest-decks && npm run ingest-metadata",
    "ingest-prices": "node --max_old_space_size=16384 --require ts-node/register src/util/ingest/commands/IngestPrices.ts",
    "ingest-cards": "ts-node src/util/ingest/commands/IngestCards.ts",
    "ingest-setlist": "ts-node src/util/ingest/commands/IngestSetList.ts",
    "ingest-sets": "ts-node src/util/ingest/commands/IngestSets.ts",
    "ingest-decklist": "ts-node src/util/ingest/commands/IngestDeckList.ts",
    "ingest-decks": "ts-node src/util/ingest/commands/IngestDecks.ts",
    "ingest-metadata": "ts-node src/util/ingest/commands/IngestMetaData.ts",
    "add-token": "ts-node src/util/services/tokenService/commands/addToken.command.ts",
    "revoke-token": "ts-node src/util/services/tokenService/commands/revokeToken.command.ts"
  },
  "author": "Milo Rue",
  "license": "ISC",
  "dependencies": {
    "@types/nodemailer": "^6.4.4",
    "apollo-server": "^3.3.0",
    "apollo-server-core": "^3.3.0",
    "apollo-tracing": "^0.15.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.2",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.17.12",
    "@types/sqlite3": "^3.1.7",
    "@types/type-is": "^1.6.3",
    "@types/uuid": "^8.3.1",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "release-it": "^14.11.5",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.2"
  }
}
