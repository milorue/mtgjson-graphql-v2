{
  "name": "mtgjson-graphql-v2",
  "version": "0.5.0",
  "description": "A GraphQL API for mtgjson.com",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "ingest-database": "npm run ingest-cards && npm run ingest-setlist && npm run ingest-sets && npm run ingest-decklist && npm run ingest-decks && npm run ingest-metadata",
    "ingest-cards": "ts-node src/util/ingest/commands/IngestCards.ts",
    "ingest-setlist": "ts-node src/util/ingest/commands/IngestSetList.ts",
    "ingest-sets": "ts-node src/util/ingest/commands/IngestSets.ts",
    "ingest-decklist": "ts-node src/util/ingest/commands/IngestDeckList.ts",
    "ingest-decks": "ts-node src/util/ingest/commands/IngestDecks.ts",
    "ingest-metadata": "ts-node src/util/ingest/commands/IngestMetaData.ts",
    "add-token": "ts-node src/util/services/tokenService/commands/addToken.command.ts",
    "revoke-token": "ts-node src/util/services/tokenService/commands/revokeToken.command.ts"
  },
  "author": "Milo Rue",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "@types/nodemailer": "^6.4.0",
    "apollo-server": "^2.19.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1",
    "validator": "^13.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.14.10",
    "@types/sqlite3": "^3.1.6",
    "@types/type-is": "^1.6.3",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "release-it": "^14.2.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
